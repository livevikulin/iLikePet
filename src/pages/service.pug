							.service__card
								.service__card-icon
									img(src="./img/service-card-5.png")
								.service__card-title Оберег
								.service__card-descr 0 отзывов
						.row
							.pagination.pagination__service
								.pagination-block
									a(href="#").pagination-block__number 1
									a(href="#").pagination-block__number.pagination-number__this 2
									a(href="#").pagination-block__number 3
									a(href="#").pagination-block__points ...
									a(href="#").pagination-block__number 72
									a(href="#").pagination-block__next Далее
							