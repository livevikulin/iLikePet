extends ../views/layouts/default
block variables
	- var title = "Личный кабинет";
	- var isMain = false;
	
block content
	main
		.service
			.container
				.breadcrumbs
					a(href="#").breadcrumbs-item Главная
					a(href="#").breadcrumbs-item Услуги
					a(href="#").breadcrumbs-this__page.breadcrumbs-item Ветеринарные услуги   /  Акушерство и гинекология
				h2.service-block__title.title-page Акушерство и гинекология
				.service-tag-line
					a(href="#" class="services-link services-link_active") акушерство и гинекология
					a(href="#" class="services-link") аллергология
					a(href="#" class="services-link") анализы
					a(href="#" class="services-link") анестезиология
					a(href="#" class="services-link") бронхоспокопия
					a(href="#" class="services-link") вакцинация
					a(href="#" class="services-link services-link_hidden") вызов врача на дом
					a(href="#" class="services-link services-link_hidden") гастроскопия
					a(href="#" class="services-link services-link_hidden") гастроэнтерология
					a(href="#" class="services-link services-link_hidden") гемодиализ
					a(href="#" class="services-link services-link_hidden") гомеопатия
					a(href="#" class="open-more js-open-more services-link" data-more="6") ...
				.service-content
					.service__left
						.filter-main
							.filter-mobile
								span.filter-mobile__text Фильтр
								span.filter-mobile__icon
									i.fas.fa-sliders-h
							.filter-block
								.filter-box
									.filter-box__item
										i.fas.fa-angle-up
										span Животные
									ul.filter-box__list
										li
											input(type="checkbox", name="dogs" id="dogs")
											label(for="dogs") Собаки
												i.fas.fa-check
										li
											input(type="checkbox", name="cats" id="cats")
											label(for="cats") Кошки
												i.fas.fa-check
										li
											input(type="checkbox", name="horses" id="horses")
											label(for="horses") Лошади
												i.fas.fa-check
										li
											input(type="checkbox", name="ferrets" id="ferrets")
											label(for="ferrets") Хорьки
												i.fas.fa-check
										li
											input(type="checkbox", name="reptiles" id="reptiles")
											label(for="reptiles") Рептилии
												i.fas.fa-check
										li
											input(type="checkbox", name="rodents" id="rodents")
											label(for="rodents") Грызуны
												i.fas.fa-check
										li
											input(type="checkbox", name="amphibians" id="amphibians")
											label(for="amphibians") Амфибии
												i.fas.fa-check
										li
											input(type="checkbox", name="birds" id="birds")
											label(for="birds") Птицы
												i.fas.fa-check
										li
											input(type="checkbox", name="exotic" id="exotic")
											label(for="exotic") Экзотика
												i.fas.fa-check
								.filter-box.filter-box__bottom
									.filter-box__item
										i.fas.fa-angle-up
										span Тип организации
									ul.filter-box__list
										li
											input(type="checkbox", name="fСommon" id="fСommon")
											label(for="fСommon") Организация
												i.fas.fa-check
										li
											input(type="checkbox", name="health" id="health")
											label(for="health") Не организация
												i.fas.fa-check
								a(href="#").filter-btn__submit.filter-btn__submit_service Применить
								a(href="#").filter-btn__reset Сбросить
					.service__right
						.row
							.service__card
								.service__card-icon
									img(src="./img/service-card.png")
								.service__card-title Оберег
								.service__card-descr 0 отзывов
							.service__card
								.service__card-icon
									img(src="./img/service-card-2.png")
								.service__card-title Оберег
								.service__card-descr 0 отзывов
							.service__card
								.service__card-icon
									img(src="./img/service-card-3.png")
